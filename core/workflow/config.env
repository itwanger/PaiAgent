# Workflow 本地调试环境配置
PYTHONUNBUFFERED=1

# 运行环境
RUNTIME_ENV=dev
SERVICE_PORT=7880

# MySQL 配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=123456
MYSQL_DB=workflow

# Redis 配置
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_EXPIRE=3600

# MinIO 配置
OSS_TYPE=s3
OSS_ENDPOINT=http://localhost:9000
OSS_ACCESS_KEY_ID=minioadmin
OSS_ACCESS_KEY_SECRET=minioadmin
OSS_BUCKET_NAME=workflow
OSS_DOWNLOAD_HOST=http://localhost:9000
OSS_TTL=157788000

# OTLP 配置 (禁用)
OTLP_ENDPOINT=127.0.0.1:4317
OTLP_ENABLE=0

# Kafka 配置 (禁用)
KAFKA_ENABLE=0
KAFKA_SERVERS=localhost:9092

# 服务地址
PLUGIN_BASE_URL=http://localhost:18888
WORKFLOW_BASE_URL=http://localhost:7880
AGENT_BASE_URL=http://localhost:17870
